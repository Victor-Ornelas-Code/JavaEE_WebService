<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>gerenciador</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <filter>
  	<filter-name>MonitoramentoFilter</filter-name>
  	<filter-class>br.com.javaee.gerenciador.servlet.MonitoramentoFilter</filter-class>  	
  </filter>
  
  <filter-mapping>
  	<filter-name>MonitoramentoFilter</filter-name>
  	<url-pattern>/entrada</url-pattern>
  </filter-mapping>
  
    <filter>
  	<filter-name>AutorizacaoFilter</filter-name>
  	<filter-class>br.com.javaee.gerenciador.servlet.AutorizacaoFilter</filter-class>  	
  </filter>
  
  <filter-mapping>
  	<filter-name>AutorizacaoFilter</filter-name>
  	<url-pattern>/entrada</url-pattern>
  </filter-mapping>
  
    <filter>
  	<filter-name>ControladorFilter</filter-name>
  	<filter-class>br.com.javaee.gerenciador.servlet.ControladorFilter</filter-class>  	
  </filter>
  
  <filter-mapping>
  	<filter-name>ControladorFilter</filter-name>
  	<url-pattern>/entrada</url-pattern>
  </filter-mapping>
  

  
</web-app>